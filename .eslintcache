es/register/register\";\r\nimport PublicRoute from \"./components/routeComponent/publicRoute\";\r\nimport PrivateRoute from \"./components/routeComponent/privateRoute\";\r\nfunction App() {\r\n  return (\r\n    <div className=\"App\">\r\n      <Switch>\r\n        <PublicRoute component={Login} path=\"/login\" />\r\n        <PublicRoute component={Register} path=\"/register\" />\r\n        <PrivateRoute component={Main} path=\"/\" exact />\r\n      </Switch>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\Onur\\Desktop\\web-apps\\ultranet-client\\src\\pages\\login\\login.tsx",["108"],"import { useMutation } from \"@apollo/client\";\r\nimport * as React from \"react\";\r\nimport { ReactComponent as Loader } from \"../../svg/loader.svg\";\r\nimport { gql } from \"@apollo/client\";\r\nimport { useHistory } from \"react-router-dom\";\r\nimport { globalContext } from \"../../store/context/global.context\";\r\nimport { stringify } from \"querystring\";\r\n\r\nconst Login: React.FC = () => {\r\n  interface IFormData {\r\n    email: string;\r\n    password: string;\r\n  }\r\n\r\n  interface IError {\r\n    [key: string]: any;\r\n  }\r\n\r\n  //////states////////\r\n\r\n  const [formData, setFormData] = React.useState<IFormData>({\r\n    email: \"\",\r\n    password: \"\",\r\n  });\r\n\r\n  const [error, setError] = React.useState<IError>({\r\n    exception: {\r\n      errors: {\r\n        email: null,\r\n        password: null,\r\n        general: null,\r\n      },\r\n    },\r\n  });\r\n\r\n  //////queries & mutations ///////////\r\n\r\n  const LOGIN = gql`\r\n    mutation LoginUser($email: String!, $password: String!) {\r\n      login(email: $email, password: $password) {\r\n        id\r\n        username\r\n        email\r\n        token\r\n      }\r\n    }\r\n  `;\r\n\r\n  ///////hooks////////////\r\n\r\n  const { dispatch } = React.useContext(globalContext);\r\n\r\n  const [login, loginResponse] = useMutation(LOGIN, {\r\n    update(proxy, result) {\r\n      const user = result.data.login;\r\n      localStorage.setItem(\"token\", user.token);\r\n      localStorage.setItem(\"user\", JSON.stringify(user));\r\n      dispatch({\r\n        type: \"USER_LOGIN\",\r\n        user,\r\n      });\r\n    },\r\n    variables: {\r\n      email: formData.email,\r\n      password: formData.password,\r\n    },\r\n    onError({ graphQLErrors }) {\r\n      if (graphQLErrors[0].extensions) setError(graphQLErrors[0].extensions);\r\n    },\r\n    onCompleted() {\r\n      history.push(\"/\");\r\n    },\r\n  });\r\n\r\n  const history = useHistory();\r\n\r\n  //////handlers//////////////////////\r\n\r\n  const inputHandler = (e: React.ChangeEvent<HTMLInputElement>) => {\r\n    setFormData({\r\n      ...formData,\r\n      [e.target.name]: e.target.value,\r\n    });\r\n  };\r\n\r\n  const submitHandler = (e: React.FormEvent<HTMLFormElement>) => {\r\n    e.preventDefault();\r\n    login();\r\n  };\r\n\r\n  const registerHandler = () => {\r\n    history.push(\"/register\");\r\n  };\r\n\r\n  return (\r\n    <div className=\"login\">\r\n      <div className=\"login__header\">\r\n        <h3>Ultranet</h3>\r\n        <p>broaden your life </p>\r\n        <p>connect and share </p>\r\n      </div>\r\n      <div className=\"login__form\">\r\n        {error.exception.errors.general && (\r\n          <span className=\"error\">{error.exception.errors.general}</span>\r\n        )}\r\n        <form name=\"login form\" onSubmit={submitHandler}>\r\n          <label htmlFor=\"email\" className=\"login__form__label\">\r\n            email\r\n            <input\r\n              type=\"email\"\r\n              id=\"email\"\r\n              className=\"login__form__input\"\r\n              name=\"email\"\r\n              onChange={inputHandler}\r\n              value={formData.email}\r\n            />\r\n            {error.exception.errors.email && (\r\n              <span className=\"error\">{error.exception.errors.email}</span>\r\n            )}\r\n          </label>\r\n          <label htmlFor=\"password\" className=\"login__form__label\">\r\n            password\r\n            <input\r\n              type=\"password\"\r\n              name=\"password\"\r\n              id=\"password\"\r\n              className=\"login__form__input\"\r\n              onChange={inputHandler}\r\n              value={formData.password}\r\n            />\r\n            {error.exception.errors.password && (\r\n              <span className=\"error\">{error.exception.errors.password}</span>\r\n            )}\r\n          </label>\r\n          <button aria-label=\"login\">\r\n            Log In\r\n            {loginResponse.loading && <Loader />}\r\n          </button>\r\n        </form>\r\n        <button aria-label=\"register\" onClick={registerHandler}>\r\n          create new account\r\n        </button>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\nexport default Login;\r\n","C:\\Users\\Onur\\Desktop\\web-apps\\ultranet-client\\src\\apollo\\client.ts",[],"C:\\Users\\Onur\\Desktop\\web-apps\\ultranet-client\\src\\api\\config.ts",[],"C:\\Users\\Onur\\Desktop\\web-apps\\ultranet-client\\src\\pages\\main\\main.tsx",[],"C:\\Users\\Onur\\Desktop\\web-apps\\ultranet-client\\src\\pages\\register\\register.tsx",[],"C:\\Users\\Onur\\Desktop\\web-apps\\ultranet-client\\src\\store\\context\\global.context.tsx",[],"C:\\Users\\Onur\\Desktop\\web-apps\\ultranet-client\\src\\store\\reducer\\userReducer.ts",[],"C:\\Users\\Onur\\Desktop\\web-apps\\ultranet-client\\src\\components\\aside\\aside.tsx",[],"C:\\Users\\Onur\\Desktop\\web-apps\\ultranet-client\\src\\components\\core\\core.tsx",[],"C:\\Users\\Onur\\Desktop\\web-apps\\ultranet-client\\src\\components\\friends\\friends.tsx",[],"C:\\Users\\Onur\\Desktop\\web-apps\\ultranet-client\\src\\utils\\utils.ts",[],"C:\\Users\\Onur\\Desktop\\web-apps\\ultranet-client\\src\\components\\routeComponent\\privateRoute.tsx",[],"C:\\Users\\Onur\\Desktop\\web-apps\\ultranet-client\\src\\components\\routeCompone